-#ifndef CONFIG_AR9100
-    tx_max_desc_per_ds_pkt = AG7100_TX_MAX_DS_LEN / tx_len_per_ds;
-    if (AG7100_TX_MAX_DS_LEN % tx_len_per_ds) tx_max_desc_per_ds_pkt++;
-#else
-    tx_max_desc_per_ds_pkt =1;
