		$ sort -s -n -k1,1 \
			<(awk '{if (NF>1) {last = strtonum($2)}; printf("%d %s\n", last, $0)}' \
				xlat/evdev_keycode.in.new) \
			| sed 's/^[0-9]* //'  > xlat/evdev_keycode.in

