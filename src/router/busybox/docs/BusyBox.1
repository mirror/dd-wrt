\&        GETOPT=`getopt -o ab:c:: --long a-long,b-long:,c-long:: \e
\&               -n 'example.busybox' -- "$@"`
\&        if [ $? != 0 ] ; then  exit 1 ; fi
\&        eval set -- "$GETOPT"
\&        while true ; do
\&         case $1 in
\&           -a|--a-long) echo "Option a" ; shift ;;
\&           -b|--b-long) echo "Option b, argument `$2'" ; shift 2 ;;
\&           -c|--c-long)
\&             case "$2" in
\&               "") echo "Option c, no argument"; shift 2 ;;
\&               *)  echo "Option c, argument `$2'" ; shift 2 ;;
\&             esac ;;
\&           --) shift ; break ;;
