    "libmount/docs/Makefile") CONFIG_FILES="$CONFIG_FILES libmount/docs/Makefile" ;;
    "libmount/docs/version.xml") CONFIG_FILES="$CONFIG_FILES libmount/docs/version.xml" ;;
