AT_SETUP([NOT])
AT_KEYWORDS([not])
PT_CHECK([ListenHTTP
	Service
		Not URL "^/echo/foo"
		Backend
			Address
			Port
		End
	End
	Service
		URL "^/echo"
		Backend
			Address
			Port
		End
	End
End
],
[GET /echo/foo
Host: example.org
end

200
x-backend-number: 1
end

GET /echo/bar
Host: example.org
end

200
x-backend-number: 0
end
])

PT_CHECK([ListenHTTP
	Service
		Not Match OR
			Header ["Host:[[:space:]]*example\\.org"]
			Host "example.net"
		End
		Backend
			Address
			Port
		End
	End
End
],
[GET /echo/foo
Host: example.com
end

200
x-backend-number: 0
end

GET /echo/foo
Host: example.org
end

503
end
])
AT_CLEANUP

