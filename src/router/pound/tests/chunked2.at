AT_SETUP([Multiple Transfer-Encoding headers])
AT_KEYWORDS([multival chunked])
PT_CHECK(
[ListenHTTP
	Service
		Backend
			Address
			Port
		End
	End
End
],
[GET /echo/foo
Transfer-Encoding: chunked
Transfer-Encoding: chunked
end

400
end
])

PT_CHECK(
[ListenHTTP
	Service
		Backend
			Address
			Port
		End
	End
End
],
[GET /echo/foo
Transfer-Encoding: chunked, flow
end

400
end
])
AT_CLEANUP
