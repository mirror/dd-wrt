@import url("/style/<% nvg("router_style"); %>/colorscheme.css");::selection{background:var(--ac-0);color:var(--tx-c)}#menuSub,#menuMainList li span,#help h2{background:var(--ac-0);border-color:var(--ac-3);color:var(--tx-c)}#menuSubList li a{background:var(--ac-1);border-color:var(--ac-4);color:var(--tx-c)}#menuSubList li a:hover,#menuSubList li a:focus-visible{background:var(--ac-2);border-color:var(--ac-5);color:var(--tx-c);outline:0}legend,.syslog_legend{color:var(--lg-c)}#help a,#help a:hover,.meter .text{color:var(--tx-c)}.meter .bar,.progressbarblock{background-color:var(--ac-0)}input[type='checkbox'],input[type='radio']{accent-color:var(--it-c)}.button,.button[disabled]:hover,::file-selector-button{background:var(--ac-0);border:1px solid var(--ac-0);color:var(--tx-c)}.button:hover,::file-selector-button:hover{background:var(--ac-2);border:1px solid var(--ac-2);color:var(--tx-c)}.button:focus,.button:focus-visible{box-shadow:0 0 5px 1px var(--ac-2)}input:focus,textarea:focus,input:focus-visible,textarea:focus-visible,select:focus-within{box-shadow:0 0 5px 1px var(--ac-2);border:1px solid var(--ac-2);outline:0}