+		break;
+	case 5:
+	case 6:
+		INFO(LOG_PREFIX " (%s): %s", inst->name, buf);
+		break;
+	case 7:
+		DEBUG(LOG_PREFIX " (%s): %s", inst->name, buf);
+		break;
+	default:
+		ERROR(LOG_PREFIX " (%s): %s", inst->name, buf);
+	}
+}
