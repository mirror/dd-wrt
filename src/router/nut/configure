		OS_NAME=$dist_cv_build_flavor
		PKG_TARGET=$(os_get_target "$dist_cv_build_flavor")
	fi

fi
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $dist_cv_build_flavor" >&5
