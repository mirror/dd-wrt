    {
        "match": {
            "left": {
                "meta": {
                    "key": "l4proto"
                }
            },
            "op": "==",
            "right": {
                "set": [
                    "tcp",
                    "udp"
                ]
            }
        }
    },
    {
        "snat": {
            "addr": {
                "map": {
                    "data": {
                        "set": [
                            [
                                {
                                    "concat": [
                                        "10.141.11.4",
                                        20
                                    ]
                                },
                                {
                                    "concat": [
                                        "192.168.2.3",
                                        80
                                    ]
                                }
                            ]
                        ]
                    },
                    "key": {
                        "concat": [
                            {
                                "payload": {
                                    "field": "saddr",
                                    "protocol": "ip"
                                }
                            },
                            {
                                "payload": {
                                    "field": "dport",
                                    "protocol": "th"
                                }
                            }
                        ]
                    }
                }
            },
            "family": "ip"
        }
    }
]

