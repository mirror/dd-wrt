                        "set": [
                            [
                                "10.141.11.4",
                                {
                                    "concat": [
                                        "192.168.2.3",
                                        80
                                    ]
                                }
                            ]
                        ]
                    },
                    "key": {
                        "payload": {
                            "field": "saddr",
                            "protocol": "ip"
                        }
                    }
                }
            },
            "family": "ip",
            "type_flags": "concat"
        }
    }
]

