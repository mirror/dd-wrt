# udp dport 53 redirect random,persistent,fully-random
[
    {
        "match": {
            "left": {
                "payload": {
                    "field": "dport",
                    "protocol": "udp"
                }
            },
	    "op": "==",
            "right": 53
        }
    },
    {
        "redirect": {
            "flags": [
                "random",
                "fully-random",
                "persistent"
            ]
        }
    }
]

